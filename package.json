        "@typescript-eslint/parser": "^2.15.0",
        "@wordpress/browserslist-config": "2.6.0",
        "@wordpress/scripts": "^6.1.1",
        "typescript": "^3.7.4"
    },
    "wp-env": {
        "plugin-dir": "custom-block-with-ts",
        "plugin-name": "custom-block-with-ts",
        "welcome-build-command": "npm run start"
